<mvc:View controllerName="logaligroup.SAPUI5.controller.Details"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core" displayBlock="true">
 
    <Page title="{i18n>detailsPageTitle}" showNavButton="true" navButtonPress=".onNavBack">
        <ObjectHeader title="{invoices>Quantity} x {invoices>ProductName}">

            <attributes>
                <ObjectAttribute text="{invoices>ProductName}"/>
                <ObjectAttribute text="{invoices>Quantity}"/>
                <ObjectAttribute text="{invoices>ExtendedPrice}"/>
                <ObjectAttribute text="{invoices>ShipperName}"/>
                <ObjectAttribute text="{invoices>ShippedDate}"/>
                <ObjectAttribute text="{invoices>Status}"/>
            </attributes>
        </ObjectHeader>
    </Page>


</mvc:View>

